{
  "name": "auth",
  "version": "1.1.0",
  "description": "This is a sample BOT Description",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --recursive -w --require ./test/bootstrap.js",
    "start": "leo test ."
  },
  "config": {
    "leo": {
      "type": "microservice",
      "name": "Leo_Auth",
      "restApiId": {
        "ref": "auth.restapi"
      },
      "aws": {
        "role": {
          "ref": "auth.roles.LeoAuthRole"
        }
      },
      "regions": [
        "us-west-2",
        "us-east-1"
      ],
      "3rdParty": true,
      "excludeProfiles": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "overrides": {
    "leo-sdk": "file:../smartshyp-deps/leo-sdk"
  },
  "dependencies": {
    "async": "^3.0.1",
    "leo-auth": "^2.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-bind": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babelify": "^10.0.0"
  }
}
